{
  "name": "@varie-ai/avatar-sdk",
  "version": "0.1.0",
  "description": "SDK for integrating Varie AI avatars into your applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "varie",
    "avatar",
    "ai",
    "spine",
    "animation",
    "character"
  ],
  "author": {
    "name": "Varie AI",
    "email": "dev@varie.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/varie-ai/avatar-sdk"
  },
  "homepage": "https://varie.ai/developers",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
